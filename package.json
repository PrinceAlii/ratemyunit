{
  "name": "ratemyunit",
  "version": "0.0.1",
  "private": true,
  "description": "A platform for university students to rate their units/subjects",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "db:migrate": "npm run migrate -w @ratemyunit/db",
    "db:seed": "npm run seed -w @ratemyunit/db",
    "db:studio": "npm run studio -w @ratemyunit/db"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "prettier": "^3.8.1",
    "turbo": "^2.8.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}