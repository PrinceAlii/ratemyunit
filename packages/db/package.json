{
  "name": "@ratemyunit/db",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./dist/schema.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "migrate": "drizzle-kit generate:pg && tsx src/migrate.ts",
    "studio": "drizzle-kit studio",
    "seed": "tsx src/seed.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@node-rs/argon2": "^2.0.2",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "postgres": "^3.4.8"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "drizzle-kit": "^0.18.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
